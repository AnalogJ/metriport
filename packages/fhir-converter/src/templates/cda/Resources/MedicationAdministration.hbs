{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "MedicationAdministration",
        "id":"{{ID}}",
        "identifier":
  		[
        	{{#each (toArray medicationAdministration.id)}}
            	{{>DataType/Identifier.hbs id=this}}
            {{/each}}
        ],
        "status":{{>ValueSet/MedicationAdministrationStatus.hbs code=medicationAdministration.statusCode.code}},
        "effectivePeriod":
        	{"start":"{{formatAsDateTime medicationAdministration.effectiveTime.low.value}}",
            "end":"{{formatAsDateTime medicationAdministration.effectiveTime.high.value}}"
        	},
        "dosage":
        {

                "route":{{>DataType/CodeableConcept.hbs code=medicationAdministration.routeCode}}
                {{!-- dose and rateQuantity are type SimpleQuantity --}}
                "dose":
                {
                    "value":"{{removeNonNumeric (decodeHtmlEntities medicationAdministration.doseQuantity.value)}}",
                    "unit":"{{medicationAdministration.doseQuantity.unit}}"
                }
                "rateQuantity":
                {
                    "value":"{{removeNonNumeric (decodeHtmlEntities medicationAdministration.rateQuantity.value)}}",
                    "unit":"{{medicationAdministration.rateQuantity.unit}}"
                }

        }
    },
    "request":{
        "method":"PUT",
        "url":"MedicationAdministration/{{ID}}",
    },
},